<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/recognition/Cim"
     xml:base="http://purl.org/recognition/Cim"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:Cim="http://purl.org/recognition/Cim#"
     xmlns:dc3="http://purl.org/dc/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:saref="https://w3id.org/saref#"
     xmlns:terms="http://purl.org/dc/terms"
     xmlns:terms1="http://purl.org/dc/terms/"
     xmlns:saref4bldg="https://w3id.org/def/saref4bldg#">
    <owl:Ontology rdf:about="http://purl.org/recognition/Cim#">
        <owl:imports rdf:resource="https://w3id.org/saref"/>
        <owl:imports rdf:resource="https://w3id.org/def/v1/saref4bldg"/>
        <dc:title>Re-cognition ontology</dc:title>
        <terms1:creator rdf:resource="https://www.iti.gr/iti/index.html"/>
        <terms1:creator>George Sidiras</terms1:creator>
        <terms1:creator>ITI</terms1:creator>
        <terms1:license rdf:resource="http://purl.org/NET/rdflicense/cc-by4.0"/>
        <rdfs:comment>RE-COGNITION system is that it will be applicable to different building types and locations with various characteristics, preserving its interoperability, though. This is realized through the development of a common vocabulary between the different system components  and it will reuse standard ontologies whenever possible</rdfs:comment>
        <owl:versionInfo>0.0.1</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/termscreator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/termscreator"/>
    


    <!-- http://purl.org/dc/termslicense -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/termslicense"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/creator"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/recognition/Cim#preferredNamespaceUri -->

    <owl:AnnotationProperty rdf:about="http://purl.org/recognition/Cim#preferredNamespaceUri"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/recognition/Cim#consumes -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#consumes">
        <rdfs:label>consumes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasComfortSetting -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasComfortSetting">
        <rdfs:label>has comfort settings</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasCurrency -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasCurrency">
        <rdfs:comment>Link between a price and its currency</rdfs:comment>
        <rdfs:label>has currency</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasDuration -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasDuration">
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>Duration of an event</rdfs:comment>
        <rdfs:label>has duration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasIncentive -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasIncentive">
        <rdfs:comment>A relation between a property and its incentives associated</rdfs:comment>
        <rdfs:label>has incentive</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasPayment -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasPayment">
        <rdfs:label>has payment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasPhase -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasPhase">
        <rdfs:label>has phase</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasProfile -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasProfile">
        <rdfs:comment>Link between an entity and its profile</rdfs:comment>
        <rdfs:label>has profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasSystemConstraint -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasSystemConstraint">
        <rdfs:label>has system constraint</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#hasTransaction -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#hasTransaction">
        <rdfs:label>has transaction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#isLocatedAt -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#isLocatedAt">
        <rdfs:comment>Relation between an object and the geographical feature in which it is located at</rdfs:comment>
        <rdfs:label>is located at</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#isOwnedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#isOwnedBy">
        <owl:inverseOf rdf:resource="http://purl.org/recognition/Cim#owns"/>
        <rdfs:comment>Link between a device and its customer</rdfs:comment>
        <rdfs:label>is owned by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#isRelatedToProperty -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#isRelatedToProperty">
        <rdfs:label>is related to property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#isValidDuringPeriod -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#isValidDuringPeriod">
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
        <rdfs:comment>Period where an entity, such a contract, is valid</rdfs:comment>
        <rdfs:label>is valid during period</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#manages -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#manages">
        <rdfs:comment>Relation between a Bulding and the  devices it manages</rdfs:comment>
        <rdfs:label>manages</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#owns -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#owns">
        <rdfs:comment>Link between the customers and the devices</rdfs:comment>
        <rdfs:label>owns</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/recognition/Cim#quantifiesKPI -->

    <owl:ObjectProperty rdf:about="http://purl.org/recognition/Cim#quantifiesKPI">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/saref4city"/>
    </owl:ObjectProperty>
    


    <!-- http://www.opengis.net/ont/geosparql#hasGeometry -->

    <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#hasGeometry">
        <rdfs:domain rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:isDefinedBy rdf:resource="http://www.opengis.net/ont/geosparql"/>
        <rdfs:isDefinedBy>A spatial representation for a given feature.</rdfs:isDefinedBy>
        <rdfs:label>hasGeometry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/def/openadr#hasDuration -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/openadr#hasDuration"/>
    


    <!-- https://w3id.org/def/openadr#sendsEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/openadr#sendsEvent"/>
    


    <!-- https://w3id.org/def/saref4city#isDerivedFrom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/def/saref4city#isDerivedFrom">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/def/saref4city"/>
        <rdfs:label>is derived from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#consistsOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#consistsOf">
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/saref"/>
        <rdfs:label>consists of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#hasFeatureOfInterest -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#hasFeatureOfInterest">
        <rdfs:isDefinedBy>https://w3id.org/saref#</rdfs:isDefinedBy>
        <rdfs:label>has feature of interest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isAbout -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isAbout">
        <rdfs:comment>A relationship identifying what an entity, such as a Profile, is about</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/saref#</rdfs:isDefinedBy>
        <rdfs:label>isAbout</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isMeasuredIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isMeasuredIn">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>is measured in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#isRelatedToMeasurement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#isRelatedToMeasurement">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>is related to measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#makesMeasurement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#makesMeasurement">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>makes measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/saref#measuresProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/saref#measuresProperty">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>measures property</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/recognition/Cim#clusterDescription -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#clusterDescription">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:comment>Description of a cluster</rdfs:comment>
        <rdfs:label>cluster description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#clusterName -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#clusterName">
        <rdfs:comment>Name of a cluster</rdfs:comment>
        <rdfs:label>cluster name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#granularity -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#granularity">
        <rdfs:comment>granularity of the profile (e.g., weekly, daily)</rdfs:comment>
        <rdfs:label>granularity</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#hasDescription -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#hasDescription">
        <rdfs:label>description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#href -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#href">
        <rdfs:comment>URI of the endpoint</rdfs:comment>
        <rdfs:label>href</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#name -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#name">
        <rdfs:isDefinedBy>name</rdfs:isDefinedBy>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#sectorName -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#sectorName">
        <rdfs:label>sector name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#status -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#status">
        <rdfs:comment>The final status as if the DVN fullfiled the request or it failed</rdfs:comment>
        <rdfs:label>status</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/recognition/Cim#type -->

    <owl:DatatypeProperty rdf:about="http://purl.org/recognition/Cim#type">
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:label>type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/saref#hasValue -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/saref#hasValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>has value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/saref#timeStamp -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/saref#timeStamp">
        <rdfs:isDefinedBy>https://w3id.org/def/saref</rdfs:isDefinedBy>
        <rdfs:label>timestamp</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/recognition/Cim#Asset -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Asset">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Device"/>
        <rdfs:label>Asset</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Asset_Configuration -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Asset_Configuration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#FeatureOfInterest"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Asset_Control -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Asset_Control">
        <owl:equivalentClass rdf:resource="https://w3id.org/saref#Command"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#ComfortSetting -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#ComfortSetting">
        <rdfs:label>Comfort setting</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Energy_Market_Price -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Energy_Market_Price"/>
    


    <!-- http://purl.org/recognition/Cim#Fluid_Level -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Fluid_Level">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Property"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Frequency -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Frequency">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Property"/>
        <rdfs:comment>Frequency constraint related to the grid</rdfs:comment>
        <rdfs:label>Frequency</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Incentives -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Incentives">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#FeatureOfInterest"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Installation_Cost -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Installation_Cost">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#FeatureOfInterest"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Price"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Measurement -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Measurement">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Measurement"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#hasPhase"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/recognition/Cim#Phase"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Operator -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Operator"/>
    


    <!-- http://purl.org/recognition/Cim#Phase -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Phase">
        <rdfs:label>Phase</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Profile -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Profile">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Profile"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/def/openadr#hasDuration"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#granularity"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2002/07/owl#rational"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A specification associated to a Building Profiles  or a device to collect information about profiles</rdfs:comment>
        <rdfs:label>Profile</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Return_Of_Investment -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Return_Of_Investment">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#FeatureOfInterest"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Sector -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Sector">
        <rdfs:subClassOf rdf:resource="http://www.opengis.net/ont/geosparql#Feature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#sectorName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A section or zone</rdfs:comment>
        <rdfs:label>Sector</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#SoC -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#SoC">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Property"/>
        <terms1:description>State of charge (SoC) is the level of charge of an electric battery relative to its capacity.</terms1:description>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Soc_Unit -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Soc_Unit">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#UnitOfMeasure"/>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#SystemConstraint -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#SystemConstraint">
        <rdfs:label>System constraint</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.org/recognition/Cim#Voltage -->

    <owl:Class rdf:about="http://purl.org/recognition/Cim#Voltage">
        <rdfs:subClassOf rdf:resource="https://w3id.org/saref#Property"/>
        <rdfs:comment>Voltage constraint related to the grid</rdfs:comment>
        <rdfs:label>Voltage</rdfs:label>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#Building_Manager -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#Building_Manager">
        <rdfs:subClassOf rdf:resource="http://www.co-ode.org/ontologies/ont.owl#Building_Operator_"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#Building_Operator_ -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#Building_Operator_">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#consumes"/>
                <owl:someValuesFrom rdf:resource="http://purl.org/recognition/Cim#Operator"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#hasProfile"/>
                <owl:allValuesFrom rdf:resource="http://purl.org/recognition/Cim#Profile"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Legal entities that aggregate the load or generation of various demand and/or generation/production units and aim at optimizing energy supply and consumption.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/ont.owl#GroupAsset -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/ont.owl#GroupAsset">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#clusterDescription"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/recognition/Cim#clusterName"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Group Asset such as FEIDs or Cluserr Devices </rdfs:comment>
